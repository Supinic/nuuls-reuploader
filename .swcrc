{
  "jsc": {
    "experimental": {
        "keepImportAssertions": true
    },
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "privateMethod": false,
      "functionBind": false,
      "exportDefaultFrom": false,
      "exportNamespaceFrom": false,
      "decorators": false,
      "decoratorsBeforeExport": false,
      "topLevelAwait": true,
      "importMeta": true
    },
    "transform": null,
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  }
}
